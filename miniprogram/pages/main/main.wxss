@font-face {
  font-family: 'webfont';
  font-display: swap;
  src: url('//at.alicdn.com/t/webfont_1hnj332vka2.eot'); /* IE9*/
  src: url('//at.alicdn.com/t/webfont_1hnj332vka2.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('//at.alicdn.com/t/webfont_1hnj332vka2.woff2') format('woff2'),
  url('//at.alicdn.com/t/webfont_1hnj332vka2.woff') format('woff'), /* chrome、firefox */
  url('//at.alicdn.com/t/webfont_1hnj332vka2.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('//at.alicdn.com/t/webfont_1hnj332vka2.svg#思源黑体-粗') format('svg'); /* iOS 4.1- */
}

page {
  height: 100%;
  display: flex;
  flex-direction: column;
}

.content {
  font-family: "webfont" !important;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.box-wrapper {
  display: flex;
  flex-wrap:wrap;
  justify-content: space-around;
}

.box {
  width: 300rpx;
  height: 400rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
}

.image {
  box-sizing: border-box;
  width: 252rpx;
  height: 252rpx;
  border-radius: 50%;
  padding: 50rpx;
}

.title {
  display: block;
}